{
  "env": {
      "es2021": true,
      "node": true
  },
  "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
  },
  "plugins": [
      "prettier"
  ],
  "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
  ],
  "rules": {
      "prettier/prettier": "error"
  }
}